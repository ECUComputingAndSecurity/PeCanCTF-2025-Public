  <!-- Popup modal for submitting challenges -->
  <div class="c-modal">
    <div class="box">
      <h1>Submission Form</h1>
      <p class="mb-4">
        Please upload a .zip file containing the source code for your
        challenge. Make sure your challenge is containerized using Docker and
        there is a writeup file as well.
      </p>
      <div>
        <!-- Username and author's email forms -->
        <div class="d-flex gap-2">
          <input type="email" class="bg-dark mt-3 form-control" placeholder="author@email.com" />
          <input type="name" class="bg-dark mt-3 form-control" placeholder="Author's name" />
        </div>

        <!-- dropdown for select which category the challenge will be -->
        <label class="category mt-3">Category</label><br />
        <div class="form-check form-check-inline">
          <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio1" value="web" />
          <label class="form-check-label" for="inlineRadio1">web</label>
        </div>
        <div class="form-check form-check-inline">
          <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio2" value="crypto" />
          <label class="form-check-label" for="inlineRadio2">crypto</label>
        </div>
        <div class="form-check form-check-inline">
          <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio2" value="osint" />
          <label class="form-check-label" for="inlineRadio2">osint</label>
        </div>
        <div class="form-check form-check-inline">
          <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio2" value="rev" />
          <label class="form-check-label" for="inlineRadio2">rev</label>
        </div>
        <div class="form-check form-check-inline">
          <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio2" value="ai" />
          <label class="form-check-label" for="inlineRadio2">AI</label>
        </div>
        <div class="form-check form-check-inline">
          <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio2" value="misc" />
          <label class="form-check-label" for="inlineRadio2">misc</label>
        </div>
        <div class="form-check form-check-inline">
          <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio2" value="mobile" />
          <label class="form-check-label" for="inlineRadio2">mobile</label>
        </div>
        <div class="form-check form-check-inline">
          <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio2" value="forensic" />
          <label class="form-check-label" for="inlineRadio2">forensic</label>
        </div>

        <!-- Drag and drop section for files -->
        <input accept="zip,application/octet-stream,application/zip,application/x-zip,application/x-zip-compressed"
          type="file" name="filechal" style="display: none" />
        <section class="mt-3 dragdrop d-flex align-content-center flex-column">
          <img src="/static/imgs/accept.png" id="loaded" />
          <img src="/static/imgs/upload.png" />
          <label id="upload-file-section">Upload your challenges here</label>
        </section>

        <!-- Submit buttons -->
        <div class="d-flex w-100 gap-2 mt-4">
          <button id="cancel" class="btn btn-outline-secondary w-100" type="button">
            Cancel
          </button>

          <button id="send_challenge" type="button" class="w-100 btn btn-primary">
            <div class="spinner-border hide" role="status">
              <span class="sr-only"></span>
            </div>
            <label style="cursor: pointer;">Submit</label>
          </button>
        </div>
      </div>
    </div>
  </div>

  <!-- This looks ugly as heck, make some changes in the future please -->
  <section class="submitbtn">
    <label>+</label>
  </section>
</body>

